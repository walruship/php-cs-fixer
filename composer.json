{
  "name": "walruship/php-cs-fixer",
  "description": "Automatically fix PHP coding standards configuration for Walruship projects.",
  "authors": [
    {
      "name": "Walruship Co., Ltd",
      "email": "contact@walruship.com",
      "homepage": "https://walruship.com"
    }
  ],
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.1",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Walruship\\PhpCsFixer\\": "src"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  },
  "minimum-stability": "stable"
}